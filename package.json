{
  "name": "times-slider",
  "version": "1.0.20",
  "type": "module",
  "description": "Nice looking and easy customize vue component for time select using slider.",
  "module": "times-slider.es.js",
  "browser": "times-slider.umd.js",
  "unpkg": "times-slider.umd.js",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "post": "cp ./package.json ./dist  && cp ./LICENSE ./dist && cp ./src/assets/slider.styl ./dist/slider.styl && cp ./README.md ./dist/README.md",
    "build:docs": "vite build --config vite.config.web.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "format": "prettier --write src/"
  },
  "exports": {
    ".": {
      "import": "times-slider.es.js",
      "require": "times-slider.umd.js"
    }
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruuva/time-slider.git"
  },
  "keywords": [
    "vue",
    "svg",
    "web-component",
    "slider",
    "input"
  ],
  "author": "Aleksey Bazhenov <alekseybazhenov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dhruuva/time-slider/issues"
  },
  "homepage": "https://dhruuva.github.io/time-slider/",
  "peerDependencies": {
    "vue": "3.x"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^27.0.1",
    "prettier": "3.6.2",
    "stylus": "^0.64.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-pug": "^0.4.1",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4"
  }
}
